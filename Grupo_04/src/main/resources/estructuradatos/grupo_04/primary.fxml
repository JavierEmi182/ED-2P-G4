<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="264.0" prefWidth="456.0" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="estructuradatos.grupo_04.PrimaryController">
   <children>
      <HBox prefHeight="110.0" prefWidth="422.0">
         <children>
            <VBox prefHeight="107.0" prefWidth="211.0">
               <children>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label prefHeight="17.0" prefWidth="90.0" text="File preguntas:" />
                        <Label fx:id="lbPreguntastxt" prefHeight="17.0" prefWidth="119.0" text="preguntas.txt" />
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Label prefHeight="15.0" prefWidth="90.0" text="File respuestas:" />
                        <Label fx:id="lbRespuestastxt" prefHeight="17.0" prefWidth="89.0" text="respuestas.txt" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <VBox prefHeight="107.0" prefWidth="205.0">
               <children>
                  <Button fx:id="btnCargarPreguntas" mnemonicParsing="false" onAction="#fileChooserPreguntas" text="Editar">
                     <VBox.margin>
                        <Insets left="20.0" />
                     </VBox.margin>
                  </Button>
                  <Button fx:id="btnCargarRespuestas" mnemonicParsing="false" onAction="#fileChooserRespuestas" text="Editar">
                     <VBox.margin>
                        <Insets left="20.0" top="25.0" />
                     </VBox.margin>
                  </Button>
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="primaryButton" onAction="#switchToSecondary" text="CrearPartida" textAlignment="CENTER">
               <HBox.margin>
                  <Insets left="150.0" top="20.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
